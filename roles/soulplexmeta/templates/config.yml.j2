libraries:
  Anime:
    collection_files:
    - pmm: basic
    - pmm: anilist

  Children Movies:
    collection_files:
    - pmm: basic
    - pmm: studio
    - pmm: genre
    - pmm: seasonal
    operations:
      mass_critic_rating_update: tmdb
      mass_audience_rating_update: tmdb
      split_duplicates: true

  Children TV Shows:
    collection_files:
    - pmm: basic
    - pmm: network

  Movies:
    collection_files:
    - pmm: basic
    - pmm: studio
    - pmm: genre
    - pmm: seasonal
    operations:
      mass_critic_rating_update: tmdb
      mass_audience_rating_update: tmdb
      split_duplicates: true

  TV Shows:
    collection_files:
    - pmm: basic
    - pmm: network

playlist_files:
  - pmm: playlist
    template_variables:
      libraries: Children Movies, Children TV Shows, Movies, TV Shows

settings:
  cache: true
  cache_expiration: 60
  asset_directory: config/assets
  asset_folders: true
  asset_depth: 0
  create_asset_folders: false
  prioritize_assets: false
  dimensional_asset_rename: false
  download_url_assets: false
  show_missing_season_assets: false
  show_missing_episode_assets: false
  show_asset_not_needed: true
  sync_mode: append
  minimum_items: 1
  default_collection_order:
  delete_below_minimum: true
  delete_not_scheduled: false
  run_again_delay: 2
  missing_only_released: false
  only_filter_missing: false
  show_unmanaged: true
  show_unconfigured: true
  show_filtered: false
  show_options: false
  show_missing: true
  show_missing_assets: true
  save_report: false
  tvdb_language: eng
  ignore_ids:
  ignore_imdb_ids:
  item_refresh_delay: 0
  playlist_sync_to_users: all
  playlist_exclude_users:
  playlist_report: false
  verify_ssl: true
  custom_repo:
  check_nightly: false

plex:
  url: {{ plexmm_plex_url }}
  token: {{ plexmm_plex_token }}
  timeout: 60
  clean_bundles: false
  empty_trash: false
  optimize: false

tautulli:
  url: {{ plexmm_tautulli_url }}
  apikey: {{ secret_tautulli_api }}

tmdb:
  apikey: {{ plexmm_tmdb_apikey }}
  language: en

trakt:
  client_id: {{ plexmm_trakt_clientid }}
  client_secret: {{ plexmm_trakt_clientsecret }}
  authorization:
    access_token:
    token_type:
    expires_in:
    refresh_token:
    scope: public
    created_at:
  pin: {{ plexmm_trakt_pin }}

webhooks:
  error: {{ plexmm_discord_webhook }}
  #changes: {{ plexmm_discord_webhook }}